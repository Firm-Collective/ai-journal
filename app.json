{
  "expo": {
    "name": "One Voice Echo",
    "slug": "one-voice-journal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.firmcollective.onevoiceecho",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "associatedDomains": ["applinks:firmcollective.org"],
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      },
      "bundleIdentifier": "com.firmcollective.OneVoiceEcho"
    },
    "android": {
      "permissions": [
        "android.permission.INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "lyton.dev",
              "pathPrefix": "/login"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.firmcollective.OneVoiceEcho"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Please allow One Voice Echo to access your Face ID biometric data for logging in."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.64478294171-ehjcv2i43egrjdcggpm3n4c0a7l5jkk9"
        }
      ]
    ],
    "experiments": {},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b150b9e0-b584-4976-8560-e1b23b830871"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ca2a4644-0c86-4092-b1f9-d554abfc52cf"
    },
    "owner": "ai-journal"
  }
}
